{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}